{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Run Desktop Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "linux"
        },
        {
            "name": "Flutter Run Desktop Profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "deviceId": "linux"
        },
        {
            "name": "Flutter Run Desktop Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "linux"
        },
        {
            "name": "Flutter Run Web Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "web-server",
            "toolArgs": ["--web-port", "3000"]
        },
        {
            "name": "Flutter Run Web Profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "deviceId": "web-server",
            "toolArgs": ["--web-port", "3000"]
        },
        {
            "name": "Flutter Run Web Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "web-server",
            "toolArgs": ["--web-port", "3000"]
        },
        {
            "name": "[UNSTABLE] Flutter Run Android Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "emulator-5554"
        },
        {
            "name": "[UNSTABLE] Flutter Run Android Profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "deviceId": "emulator-5554"
        },
        {
            "name": "[UNSTABLE] Flutter Run Android Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "emulator-5554"
        },
        {
            "name": "Python Django Debug",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            // "justMyCode": false,
            "program": "${workspaceFolder}/manage.py"
        }
    ]
}